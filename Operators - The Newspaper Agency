import java.util.*;

public class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);

        int a = sc.nextInt(); 
        int b = sc.nextInt(); 
        int c = sc.nextInt(); 

        int revenue = a * b;
        int cost = (a * c) + 100; 
        int profit = revenue - cost;

        System.out.println(profit);
    }
}
