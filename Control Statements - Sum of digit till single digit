import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int num = sc.nextInt();

        int result;
        if (num == 0) {
            result = 0;
        } else {
            result = 1 + ((num - 1) % 9);
        }
        System.out.println( result);
    }
}
